<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sqliterecyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SQLiteRecyclerView">
        <activity android:name=".EduUpdateActivity" android:exported="true"/>

        <activity
            android:name=".UpdateActivity"
            android:exported="true" />
        <activity
            android:name=".RecyclerView"
            android:exported="true"
            android:parentActivityName=".CandidateEducation" />

        <activity android:name=".EduRecyclerView" android:exported="true" android:parentActivityName=".CandidateEducation"/>
        <activity android:name=".CandidateEducation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>